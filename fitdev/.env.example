# FitDev.io Environment Configuration

# Enable/disable LLM capabilities
ENABLE_LLM=false
ENABLE_BROWSER=false

# LLM Provider Configuration
DEFAULT_LLM_PROVIDER=ollama  # Options: openai, ollama

# OpenAI Configuration
OPENAI_API_KEY=your_openai_api_key_here
OPENAI_MODEL=gpt-3.5-turbo

# Ollama Configuration
OLLAMA_API_BASE=http://localhost:11434
OLLAMA_MODEL=llama3:latest

# Organization Settings
ORGANIZATION_NAME=FitDev.io
MAX_CYCLES=5

# Logging
LOG_LEVEL=INFO

# Reinforcement Learning Settings
ENABLE_LEARNING=false
LEARNING_DATA_DIR=data/learning

# Learning Parameters
PARAMETER_LEARNING_RATE=0.05
STRATEGY_EXPLORATION_RATE=0.2
